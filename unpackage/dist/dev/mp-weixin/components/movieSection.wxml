<view><navigator class="movie-section" url="{{url}}"><image class="movie-img" src="{{movie.img}}"></image><view class="movie-info"><view class="movie-name line-ellipsis">{{movie.nm+''}}<text class="{{['version',movie.version]}}"></text></view><view class="movie-score line-ellipsis"><block wx:if="{{movie.globalReleased}}"><view><block wx:if="{{movie.sc}}"><text>观众评<text class="grade">{{movie.sc}}</text></text></block><block wx:else><text>暂无评分</text></block></view></block><block wx:else><view><text class="grade">{{movie.wish}}</text>人想看</view></block></view><view class="movie-star line-ellipsis">{{"主演："+movie.star}}</view><block wx:if="{{movie.rt}}"><view>{{movie.rt+"上映"}}</view></block><block wx:else><view class="movie-showInfo line-ellipsis">{{movie.showInfo}}</view></block></view><view class="buy-tickets"><block wx:if="{{movie.showst===3}}"><view class="btn">购票</view></block><block wx:else><block wx:if="{{movie.showst===1}}"><view class="btn want-see">想看</view></block><block wx:else><block wx:if="{{movie.showst===4}}"><view class="btn pre-sale">预售</view></block></block></block></view></navigator></view>