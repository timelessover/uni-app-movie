<view class="container" style="{{'position:'+(isShow?'fixed':'')+';'}}"><view class="top"><view><select-time vue-id="1" start-time="{{showTime}}" data-event-opts="{{[['^selectDayEvent',[['changeTime']]]]}}" bind:selectDayEvent="__e" bind:__l="__l"></select-time></view><view><filter-nav hidden="{{!(isShow)}}" vue-id="2" city-cinema-info="{{cityCinemaInfo}}" data-event-opts="{{[['^change',[['changeCondition']]],['^toggleShow',[['toggleShow']]]]}}" bind:change="__e" bind:toggleShow="__e" bind:__l="__l"></filter-nav></view></view><view class="main-content"><view class="cinema-list"><block wx:for="{{cinemas}}" wx:for-item="cinema" wx:for-index="index" wx:key="id"><cinema-section vue-id="{{'3-'+index}}" cinema="{{cinema}}" bind:__l="__l"></cinema-section></block></view><block wx:if="{{!loadComplete&&cinemas.length}}"><view><loading-more vue-id="4" bind:__l="__l"></loading-more></view></block><view hidden="{{!(nothing)}}"><empty vue-id="5" message="暂无符合条件的影院" bind:__l="__l"></empty></view><view hidden="{{!(noSchedule)}}"><empty vue-id="6" message="当天暂无场次" bind:__l="__l"></empty></view></view></view>